{
    "$schema": "https://flagd.dev/schema/v0/flags.json",
    "flags": {
        "use-new-counter-version": {
            "state": "ENABLED",
            "defaultVariant": "false",
            "variants": {
                "true": true,
                "false": false
            },
            "targeting": {}
        },
        "return-weather-forecast": {
            "state": "ENABLED",
            "defaultVariant": "five",
            "variants": {
                "five": 5,
                "three": 3
            },
            "targeting": {}
        },
        "page-v2": {
            "state": "DISABLED",
            "defaultVariant": "old-page",
            "variants": {
                "new-page": true,
                "old-page": false
            },
            "targeting": {
                "if": [
                    {
                        "==": [
                            {
                                "var": "country"
                            },
                            "pt"
                        ]
                    },
                    "new-page"
                ]
            }
        }
    }
}